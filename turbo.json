{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "prisma:push": {
      "cache": false
    },
    "prisma:force-push": {
      "cache": false
    },
    "prisma:seed": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
