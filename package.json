{
  "name": "projectname",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "build:api": "turbo run build --filter=@projectname/api",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prisma:push": "turbo run prisma:push --filter=@projectname/api",
    "prisma:seed": "turbo run prisma:seed --filter=@projectname/api"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.8.1",
    "turbo": "^2.8.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@10.0.0"
}
